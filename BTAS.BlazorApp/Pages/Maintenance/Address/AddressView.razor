@code {
    [Parameter]
    public int id { get; set; }
    private EditContext? editContext;
    private tbl_client_headerDto address = new();
    private List<tbl_client_contact_detailsDto> contacts = new();
    private List<tbl_hawb_incotermsDto> incoterms = new();
    private bool formInvalid = false;
    private bool HideElement { get; set; } = false;
    private ResponseDto responseDto = new();
    private List<BreadcrumbItem> _items = new List<BreadcrumbItem>
{
        new BreadcrumbItem("Home", href: "/"),
        new BreadcrumbItem("Address Book", href: "/address"),
        new BreadcrumbItem("Address Book View", href: "/address/view")

    };

    protected override async Task OnParametersSetAsync()
    {
        if (id != 0)
        {
            var headerResult = await AddressService.GetClientHeaderByIdAsync<ResponseDto>(id);
            var jsonSerializerSettings = new JsonSerializerSettings();
            jsonSerializerSettings.MissingMemberHandling = MissingMemberHandling.Ignore;
            var jsonString = JsonConvert.SerializeObject(headerResult.Result, jsonSerializerSettings);

            if (headerResult.Result != null && headerResult.IsSuccess)
            {
                address = JsonConvert.DeserializeObject<tbl_client_headerDto>(jsonString, jsonSerializerSettings);
            }

            if (headerResult.IsSuccess)
                HideElement = !HideElement;
        }
        else
        {
            HideElement = !HideElement;
        }
    }

    protected override async Task OnInitializedAsync()
    {
        editContext = new(address);

        editContext.OnFieldChanged += HandleFieldChanged;

        //var result = await ContactService.GetAllContactDetailsAsync<ResponseDto>();
        //jsonString = JsonConvert.SerializeObject(result.Result, jsonSerializerSettings);

        //if (jsonString != "[]" && result.IsSuccess)
        //    contacts = JsonConvert.DeserializeObject<List<tbl_client_contact_detailsDto>>(jsonString, jsonSerializerSettings);

        //var incotermsResult = await IncotermsService.GetAllIncotermsAsync<ResponseDto>();
        //jsonString = JsonConvert.SerializeObject(incotermsResult.Result, jsonSerializerSettings);

        //if (jsonString != "[]" && incotermsResult.IsSuccess)
        //    incoterms = JsonConvert.DeserializeObject<List<tbl_hawb_incotermsDto>>(jsonString, jsonSerializerSettings);

        await base.OnInitializedAsync();
    }

    private void HandleFieldChanged(object? sender, FieldChangedEventArgs e)
    {
        if (editContext is not null)
        {
            formInvalid = !editContext.Validate();
            StateHasChanged();
        }
    }

    private async Task HandleValidSubmit(EditContext context)
    {
        if (context.Validate())
        {
            if (address.idtbl_client_header != 0)
            {
                responseDto = await AddressService.CreateClientHeaderAsync<ResponseDto>(address);
            }
            else
            {
                responseDto = await AddressService.UpdateClientHeaderAsync<ResponseDto>(address);
            }
            Cancel();
        }
    }

    public void Cancel()
    {
        NavigationManager.NavigateTo("/address");
    }

    public void Dispose()
    {
        if (editContext is not null)
        {
            editContext.OnFieldChanged -= HandleFieldChanged;
        }
    }
}

