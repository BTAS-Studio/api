@using System.ComponentModel

<div id="hawb" class="col-md-12 app-inner-layout__content">
    <div class="col-md-8 pb-5 pl-5 pr-5 pt-3 card">
        <h4>HAWB</h4>
        <div class="p-3">
            Methods for HAWB operations
        </div>
        <div>
            <table class="responsive table table-light table-striped small">
                <thead>
                    <tr>
                        <th class=" col-md-3">Request Body Schema</th>
                        <th class=" col-md-3">Data Type</th>
                        <th class=" col-md-6">Descriptions</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var p in ViewBag.hawb)
                    {
                        <tr class="line-height-1">
                            <td class="align-top">
                                @p.name
                            </td>
                            <td class="align-top">
                                @if (p.type.IndexOf("Dto") > -1)
                                {
                                    <p class="m-1">Json array of @p.name request parameters. <a href="#@p.name.Substring(0,p.name.Length-1)">See definition</a></p>
                                }
                                else
                                {
                                    <p class="m-1">@p.type</p>
                                }
                            </td>
                            <td class="align-top">
                                @if (!p.isOptional)
                                {
                                    <div class="m-1"><b><i>(Optional)</i></b></div>
                                }
                                <div class="m-1">@p.description</div>
                                @if (p.values != null)
                                {
                                    <div class="m-1 font-italic bg-dark-gray">
                                        Enum:
                                        @foreach (var v in p.values)
                                        {
                                            <span class="badge badge-dark">"@v"</span>
                                        }
                                    </div>
                                }
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
        <div class="dropdown-divider"></div>
        <h6>HAWB Items Parameter Definitions</h6>
        <div id="hawbItems" class="pt-4">
            <table class="responsive table table-light table-striped small">
                <thead>
                    <tr>
                        <th class=" col-md-3">Request Body Schema</th>
                        <th class=" col-md-3">Data Type</th>
                        <th class=" col-md-6">Descriptions</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var p in ViewBag.hawbItems)
                    {
                        <tr class="line-height-1">
                            <td class="align-top">
                                @p.name
                            </td>
                            <td class="align-top">
                                @if (p.type.IndexOf("Dto") > -1)
                                {
                                    <p class="m-1">Json array of @p.name request parameters</p>
                                }
                                else
                                {
                                    <p class="m-1">@p.type</p>
                                }
                            </td>
                            <td class="align-top">
                                @if (p.isOptional)
                                {
                                    <div class="m-1"><b><i>(Optional)</i></b></div>
                                }
                                <div class="m-1">@p.description</div>
                                @if (p.values != null)
                                {
                                    <div class="m-1 font-italic bg-dark-gray">
                                        Enum:
                                        @foreach (var v in p.values)
                                        {
                                            <span class="badge badge-dark">"@v"</span>
                                        }
                                    </div>
                                }
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
