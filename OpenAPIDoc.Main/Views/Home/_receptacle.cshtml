@using System.ComponentModel

<div id="receptacle" class="col-md-12 app-inner-layout__content">
    <div class="col-md-8 pb-5 pl-5 pr-5 pt-3 card">
        <h4>Receptacle</h4>
        <div class="p-3">
            Methods for Receptacle operations
        </div>
        <div>
            <table class="responsive table table-light table-striped small">
                <thead>
                    <tr>
                        <th class=" col-md-3">Request Body Schema</th>
                        <th class=" col-md-3">Data Type</th>
                        <th class=" col-md-6">Descriptions</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var p in ViewBag.receptacle)
                    {
                        <tr class="line-height-1">
                            <td class="align-top">
                                @p.name
                            </td>
                            <td class="align-top">
                                @if (p.type.IndexOf("Dto") > -1)
                                {
                                    <p class="m-1">Json array of @p.name request parameters</p>
                                }
                                else
                                {
                                    <p class="m-1">@p.type</p>
                                }
                            </td>
                            <td class="align-top">
                                @if (p.isOptional)
                                {
                                    <div class="m-1"><b><i>(Optional)</i></b></div>
                                }
                                <div class="m-1">@p.description</div>
                                @if (p.values != null)
                                {
                                    <div class="m-1 font-italic bg-dark-gray">
                                        Enum:
                                        @foreach (var v in p.values)
                                        {
                                            <span class="badge badge-dark">"@v"</span>
                                        }
                                    </div>
                                }
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
    <div class="p-2 col-md-4 text-light">
        <div id="receptaclecreate" class="bg-dark p-3 method">
            <h6>Create New Receptacle</h6>
            <div><button class="badge badge-light">PAYLOAD</button></div>
            <div class="dropdown-divider"></div>
            <div>
                <button class="badge badge-info">POST</button><span class="float-right font-weight-bold">/api/receptacle/create</span>
            </div>
            <div>
                <span>Content Type: </span><span class="float-right font-weight-bold">application/json</span>
            </div>
            <div class="m-3"></div>
            <div>
                <button type="button" aria-expanded="true" aria-controls="exampleAccordion1" data-toggle="collapse" href="#receptacleCreateReq" class="btn btn-primary">
                    Sample Request
                </button>
                <div class="collapse" id="receptacleCreateReq">
                    <div class="card card-body bg-dark">
                        <pre class="text-light font-italic">
{
    "ShipperId": "AUSTSYD",
    "Mode": "",
    "Type": "",
    "NumberOfItems": 0,
    "Weight": 0,
    "Origin": "",
    "Destination": "",
    "CreationDate": "2022-03-28T13:55:16.846Z",
    "Length": 0,
    "Width": 0,
    "Height": 0
}
                </pre>
                    </div>
                </div>
            </div>
            <div class="m-3"></div>
            <div>
                <b>Success Response</b> - <span>200 <i>(indicates that the request was processed successfully.)</i></span>
            </div>
            <div class="m-3"></div>
            <div>
                <button type="button" aria-expanded="true" aria-controls="exampleAccordion1" data-toggle="collapse" href="#receptacleCreateRes" class="btn btn-secondary">
                    Sample Response
                </button>
                <div class="collapse" id="receptacleCreateRes">
                    <div class="card card-body bg-dark">
                        <pre class="text-light font-italic">
{
    "success": true,
    "response": 200,
    "referenceNumber": "RAUSTSYD0000000002",
    "responseDescription": "Receptacle successfully created."
}
                </pre>
                    </div>
                </div>
            </div>
        </div>
        <div id="receptacleupdate" class="bg-dark p-3 method">
            <h6>Update Existing Receptacle</h6>
            <div><button class="badge badge-light">PAYLOAD</button></div>
            <div class="dropdown-divider"></div>
            <div>
                <button class="badge badge-info">PUT</button><span class="float-right font-weight-bold">/api/receptacle/update</span>
            </div>
            <div>
                <span>Content Type: </span><span class="float-right font-weight-bold">application/json</span>
            </div>
            <div class="m-3"></div>
            <div>
                <button type="button" aria-expanded="true" aria-controls="exampleAccordion1" data-toggle="collapse" href="#receptacleUpdateReq" class="btn btn-primary">
                    Sample Request
                </button>
                <div class="collapse" id="receptacleUpdateReq">
                    <div class="card card-body bg-dark">
                        <pre class="text-light font-italic">
{
    "ShipperId": "AUSTSYD",
    "ReferenceNumber": "RAUSTSYD0000000001",
    "Mode": "",
    "Type": "",
    "NumberOfItems": 0,
    "Weight": 0.00,
    "Status": "OPEN",
    "Origin": "",
    "Destination": "",
    "CreationDate": "2022-03-28T13:55:16.846",
    "Length": 0.00,
    "Width": 0.00,
    "Height": 0.00,
    "HawbId": 1,
    "HawbNumber": "HAUSTSYD0000000001"
}
                </pre>
                    </div>
                    <div>
                        <p>ShipperId - Shipper Id of mawb to be linked for cross checking purpose</p>
                        <p>ReferenceNumber - Receptacle reference number to be updated</p>
                        <p>HawbNumber (optional) - Specify a HAWB reference number where request receptacle will be linked to</p>
                    </div>
                </div>
            </div>
            <div class="m-3"></div>
            <div>
                <b>Success Response</b> - <span>200 <i>(indicates that the request was processed successfully.)</i></span>
            </div>
            <div class="m-3"></div>
            <div>
                <button type="button" aria-expanded="true" aria-controls="exampleAccordion1" data-toggle="collapse" href="#receptacleUpdateRes" class="btn btn-secondary">
                    Sample Response
                </button>
                <div class="collapse" id="receptacleUpdateRes">
                    <div class="card card-body bg-dark">
                        <pre class="text-light font-italic">
{
    "success": true,
    "response": 200,
    "referenceNumber": "RAUSTSYD0000000001",
    "responseDescription": "Receptacle # RAUSTSYD0000000001 successfully updated."
}
                </pre>
                    </div>
                </div>
            </div>
        </div>
        <div id="receptaclegetall" class="bg-dark p-3 method">
            <h6>Get All Receptacle</h6>
            <div class="dropdown-divider"></div>
            <div>
                <button class="badge badge-pill badge-warning">GET</button><span class="float-right font-weight-bold">/api/receptacle</span>
            </div>
            <div>
                <span>Content Type: </span><span class="float-right font-weight-bold">application/json</span>
            </div>
            <div class="m-3"></div>
            <div>
                <b>Success Response</b> - <span>200 <i>(indicates that the request was processed successfully.)</i></span>
            </div>
            <div class="m-3"></div>
            <div>
                <b>Response: </b>
                <p><i>List of all Receptacle records in Json format.</i></p>
            </div>
        </div>
        <div id="receptaclegetbyhawb" class="bg-dark p-3 method">
            <h6>Search by HAWB</h6>
            <div class="dropdown-divider"></div>
            <div>
                <button class="badge badge-pill badge-warning">GET</button><span class="float-right font-weight-bold">/api/receptacle/getbyhawb?referenceNumber={hawb number}</span>
            </div>
            <div>
                <span>Content Type: </span><span class="float-right font-weight-bold">application/json</span>
            </div>
            <div class="m-3"></div>
            <div>
                <b>Parameter Request: </b>
                <span>referenceNumber <i>(HAWB reference number)</i></span>
            </div>
            <div class="m-3"></div>
            <div>
                <b>Success Response</b> - <span>200 <i>(indicates that the request was processed successfully.)</i></span>
            </div>
            <div class="m-3"></div>
            <div>
                <button type="button" aria-expanded="true" aria-controls="exampleAccordion1" data-toggle="collapse" href="#receptacleSearchRes" class="btn btn-secondary">
                    Sample Response
                </button>
                <div class="collapse" id="receptacleSearchRes">
                    <div class="card card-body bg-dark">
                        <pre class="text-light font-italic">
{
    "isSuccess": true,
    "result": [
        {
            "ShipperId": "AUSTSYD",
            "ReferenceNumber": "RAUSTSYD0000000001",
            "Mode": "",
            "Type": "",
            "NumberOfItems": 0,
            "Weight": 0.00,
            "Status": "OPEN",
            "Origin": "",
            "Destination": "",
            "CreationDate": "2022-03-28T13:55:16.846",
            "Length": 0.00,
            "Width": 0.00,
            "Height": 0.00,
            "HawbId": 1,
            "HawbNumber": "HAUSTSYD0000000001",
            "HAWB": null,
            "parcelInfos": []
        }
    ],
    "id": 0,
    "referenceNumber": null,
    "displayMessage": "",
    "errorMessages": null
}
                </pre>
                    </div>
                    <div>
                        <p>ShipperId - Shipper Id of mawb to be linked for cross checking purpose</p>
                        <p>ReferenceNumber - Receptacle reference number searched</p>
                        <p>HawbNumber - HAWB reference number searched which are link to receptacle</p>
                    </div>
                </div>
            </div>
        </div>
        <div id="receptaclelinktohawb" class="bg-dark p-3 method">
            <h6>Link To HAWB</h6>
            <div class="dropdown-divider"></div>
            <div>
                <button class="badge badge-pill badge-info">POST</button><span class="float-right font-weight-bold">/api/receptacle/linktohawb</span>
            </div>
            <div>
                <span>Content Type: </span><span class="float-right font-weight-bold">application/json</span>
            </div>
            <div class="m-3"></div>
            <div>
                <button type="button" aria-expanded="true" aria-controls="exampleAccordion1" data-toggle="collapse" href="#receptacleLinkToHawbReq" class="btn btn-primary">
                    Sample Request
                </button>
                <div class="collapse" id="receptacleLinkToHawbReq">
                    <div class="card card-body bg-dark">
                        <pre class="text-light font-italic">
{
  "shipperId": "AUSTSYD",
  "parentReference": "HAUSTSYD00000001",  
  "isCreateParent": false,
  "hawb" : {}, 
  "referencesToLink": [
    "RAUSTSYD00000002"  
  ]
}
                </pre>
                    </div>
                    <div>
                        <p>shipperId - Shipper Id of mawb to be linked for cross checking purpose</p>
                        <p>parentReference - HAWB reference number where specified Receptacle in referencesToLink will be linked to</p>
                        <p>hawb (optional) - Specify a voyage body request here if isCreateParent is set to TRUE</p>
                        <p>isCreateParent - Specify if new HAWB will be created and linked to specified Receptacle in referencesToLink</p>
                        <p>referencesToLink - Json array of string consisting of Receptacle reference numbers to be linked to provided HAWB reference number</p>
                    </div>
                </div>
            </div>
            <div class="m-3"></div>
            <div>
                <b>Success Response</b> - <span>200 <i>(indicates that the request was processed successfully.)</i></span>
            </div>
            <div class="m-3"></div>
            <div>
                <button type="button" aria-expanded="true" aria-controls="exampleAccordion1" data-toggle="collapse" href="#receptacleLinkToHawbRes" class="btn btn-secondary">
                    Sample Response
                </button>
                <div class="collapse" id="receptacleLinkToHawbRes">
                    <div class="card card-body bg-dark">
                        <pre class="text-light font-italic">
{
    "isSuccess": true,
    "result": [],
    "id": 0,
    "referenceNumber": "HAUSTSYD00000001",
    "displayMessage": "Receptacle successfully updated with HAWB # HAUSTSYD00000001",
    "errorMessages": null
}
                </pre>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>